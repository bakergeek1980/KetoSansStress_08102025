{
  "expo": {
    "name": "KetoSansStress",
    "slug": "keto-sans-stress",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "ketosansstress",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#2D4A22"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ketosansstress.app",
      "associatedDomains": ["applinks:ketosansstress.app"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#2D4A22"
      },
      "package": "com.ketosansstress.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "ketosansstress"
            },
            {
              "scheme": "https",
              "host": "ketosansstress.app"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow KetoSansStress to access your camera to scan meals."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow KetoSansStress to access your photo library to select meal images."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}